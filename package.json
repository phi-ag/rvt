{
  "type": "module",
  "name": "@phi-ag/rvt",
  "version": "0.2.3",
  "license": "MIT",
  "description": "Parse Revit file format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/rvt.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "revit"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./deno": "./dist/deno.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/deno": "2.5.0",
    "@types/node": "24.10.0",
    "@vitest/coverage-v8": "4.0.8",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.3",
    "vitest": "4.0.8"
  },
  "packageManager": "pnpm@10.21.0+sha512.da3337267e400fdd3d479a6c68079ac6db01d8ca4f67572083e722775a796788a7a9956613749e000fac20d424b594f7a791a5f4e2e13581c5ef947f26968a40"
}
