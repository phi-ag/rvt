{
  "type": "module",
  "name": "@phi-ag/rvt",
  "version": "0.2.3",
  "license": "MIT",
  "description": "Parse Revit file format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/rvt.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "revit"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./deno": "./dist/deno.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/deno": "2.5.0",
    "@types/node": "25.0.9",
    "@vitest/coverage-v8": "4.0.17",
    "eslint": "9.39.2",
    "globals": "17.0.0",
    "prettier": "3.8.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.53.0",
    "vitest": "4.0.17"
  },
  "packageManager": "pnpm@10.28.0+sha512.05df71d1421f21399e053fde567cea34d446fa02c76571441bfc1c7956e98e363088982d940465fd34480d4d90a0668bc12362f8aa88000a64e83d0b0e47be48"
}
