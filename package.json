{
  "type": "module",
  "name": "@phi-ag/rvt",
  "version": "0.2.1",
  "license": "MIT",
  "description": "Parse Revit file format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/rvt.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "revit"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./deno": "./dist/deno.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.30.1",
    "@trivago/prettier-plugin-sort-imports": "5.2.2",
    "@types/deno": "2.3.0",
    "@types/node": "24.0.12",
    "@vitest/coverage-v8": "3.2.4",
    "eslint": "9.30.1",
    "globals": "16.3.0",
    "prettier": "3.6.2",
    "typescript": "5.8.3",
    "typescript-eslint": "8.36.0",
    "vitest": "3.2.4"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}
