{
  "type": "module",
  "name": "@phi-ag/rvt",
  "version": "0.2.3",
  "license": "MIT",
  "description": "Parse Revit file format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/rvt.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "revit"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./deno": "./dist/deno.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@trivago/prettier-plugin-sort-imports": "6.0.2",
    "@types/deno": "2.5.0",
    "@types/node": "25.1.0",
    "@vitest/coverage-v8": "4.0.18",
    "eslint": "9.39.2",
    "globals": "17.2.0",
    "prettier": "3.8.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.54.0",
    "vitest": "4.0.18"
  },
  "packageManager": "pnpm@10.28.2+sha512.41872f037ad22f7348e3b1debbaf7e867cfd448f2726d9cf74c08f19507c31d2c8e7a11525b983febc2df640b5438dee6023ebb1f84ed43cc2d654d2bc326264"
}
