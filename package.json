{
  "type": "module",
  "name": "@phi-ag/rvt",
  "version": "0.2.3",
  "license": "MIT",
  "description": "Parse Revit file format",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phi-ag/rvt.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "revit"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./node": "./dist/node.js",
    "./deno": "./dist/deno.js"
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "lint": "eslint .",
    "test": "vitest --run",
    "test:dev": "vitest",
    "test:coverage": "vitest --run --coverage src",
    "bench": "pnpm vitest bench --run",
    "format": "prettier --write \"**/*.{js,ts,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,ts,json,yml,yaml}\"",
    "check": "pnpm format:check && pnpm lint && pnpm typecheck && pnpm test"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@trivago/prettier-plugin-sort-imports": "6.0.0",
    "@types/deno": "2.5.0",
    "@types/node": "24.10.1",
    "@vitest/coverage-v8": "4.0.13",
    "eslint": "9.39.1",
    "globals": "16.5.0",
    "prettier": "3.6.2",
    "typescript": "5.9.3",
    "typescript-eslint": "8.47.0",
    "vitest": "4.0.13"
  },
  "packageManager": "pnpm@10.23.0+sha512.21c4e5698002ade97e4efe8b8b4a89a8de3c85a37919f957e7a0f30f38fbc5bbdd05980ffe29179b2fb6e6e691242e098d945d1601772cad0fef5fb6411e2a4b"
}
